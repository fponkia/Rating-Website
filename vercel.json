{
    "version": 2,
    "functions": {
    "api/*.php": { "runtime": "vercel-php@0.7.1" }
    },
    "builds": [
        {
          "src": "public/**/*",
          "use": "@vercel/static"
        }
      ],
    "routes": [
        { "src": "/api/(.*)",  "dest": "/api/$1" },
        { "src": "/(.*)", "dest": "/index.html"},
        { "src": "/public/(.*)", "dest": "/public/$1"}]        
}
